    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@keplr-wallet/types": "^0.8.4",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
